{
  "name": "maetech-conecta-monorepo",
  "private": true,
  "packageManager": "npm@10.2.4",
  "scripts": {
    "dev:web": "turbo run dev --filter=web --",
    "dev:mobile": "turbo run dev --filter=mobile",
    "dev": "npm-run-all --parallel dev:*"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "turbo": "^2.0.9",
    "typescript": "^5",
    "npm-run-all": "^4.1.5"
  }
}
